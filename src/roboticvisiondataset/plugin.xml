<library path="src">
    <class name="Hand-Eye-Calibration" type="rqt_handeye_calibration.New_HandeyeCalibration" base_class_type="rqt_gui_py::Plugin">
        <description>
            GUI for interactive hand-eye calibration.
        </description>
        <qtgui>
            <group>
                <label>Robot Tools</label>
            </group>
            <label>Hand-Eye-Calibration</label>
            <icon type="theme">system-help</icon>
            <statustip>Interactive hand-eye calibration system.</statustip>
        </qtgui>
    </class>

    <class name="Get-Point-Coordinate" type="point_coordinate.GetPointcoordinate" base_class_type="rqt_gui_py::Plugin">
        <description>
            GUI for interactive getting points coordinates.
        </description>
        <qtgui>
            <group>
                <label>Robot Tools</label>
            </group>
            <label>Get-Point-Coordinate</label>
            <icon type="theme">system-help</icon>
            <statustip>Interactive Get-Point-Coordinate system.</statustip>
        </qtgui>
    </class>
</library>
